[Unit]
Description=COACH DNSMasq Docker Container for Provisioning Service
After=mnt-ceph-fs.service docker.service
Requires=mnt-ceph-fs.service docker.service
 
[Service]
Restart=always
ExecStart=/usr/bin/docker start provisioner_dnsmasq
ExecStop=/usr/bin/docker stop provisioner_dnsmasq
 
[Install]
WantedBy=multi-user.target
